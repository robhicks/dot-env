{
  "name": "dot-env",
  "version": "0.9.0",
  "description": "dot-env reads one or more .env files and adds the contents of them to process.env.",
  "engines": {
    "node": ">8"
  },
  "main": "index.cjs",
  "module": "index.mjs",
  "scripts": {
    "postinstall": "npx rollup -c",
    "test": "uvu tests",
    "test:coverage": "c8 --include=src yarn test",
    "test:report": "c8 report --reporter=text-lcov > coverage.lcov",
    "test:watch": "uvu tests -w"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    
    "c8": "^7.7.1",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "uvu": "^0.5.1",
    "uvu-watch": "^1.0.11"
  },
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "rollup": "^2.45.1"
  }
}
